<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					<g:Label styleName="{style.important}" ui:field="labelScreen" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Label styleName="{style.important}" ui:field="labelStorage" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Label styleName="{style.important}" ui:field="labelCanvas" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Button styleName="{style.important}" ui:field="buttonRefresh" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Button styleName="{style.important}" ui:field="buttonGetListLexiques" ui:text="getList from server"/>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 
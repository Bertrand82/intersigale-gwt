<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel visible="false">
		<g:DialogBox ui:field='dialogBox' autoHide="true" modal="true">
			
			<g:HTMLPanel>
				<g:Label  ui:field="label" ui:text=""/>
				<g:TextBox ui:field="textBox" ui:text="" visible="false"/>
				<g:Button ui:field='buttonCancel' >Cancel</g:Button>
				<g:Button ui:field='buttonOk'>Okay</g:Button>
			</g:HTMLPanel>
		</g:DialogBox>
	</g:HTMLPanel>
</ui:UiBinder> 
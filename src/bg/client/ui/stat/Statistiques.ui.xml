<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					<g:Label styleName="{style.important}" ui:field="labelTitle"
						ui:text="Statistics" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Label styleName="{style.important}" ui:field="labeNombreDeQuestions"
						ui:text="Number of questions" />
				</td>
			</tr>
			<tr>
				<td>
					<g:SimplePanel width="200px" height="200px" ui:field="panelCanvas" />

				</td>
			</tr>
			<tr>
				<td>
					<g:Button styleName="{style.important}" ui:field="buttonRefresh" ui:text="Refresh"/>

				</td>
			</tr>
		</table>

	</g:HTMLPanel>
</ui:UiBinder> 
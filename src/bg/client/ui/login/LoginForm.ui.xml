<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<table>
		<tr> <td>Email </td> <td> <g:TextBox ui:field="textBoxEmail"/> </td> </tr>
		<tr> <td>Password </td> <td><g:PasswordTextBox ui:field="password" /></td> </tr>
		<tr> <td> </td> <td><g:Button styleName="{style.important}" ui:field="buttonLogin2" text="Login" /></td> </tr>
		<tr> <td> </td> <td><g:Button  ui:field="buttonRegister" text="Register" /></td> </tr>
		</table>
		
	</g:HTMLPanel>
</ui:UiBinder> 